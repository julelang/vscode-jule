{
  "name": "vscode-jule",
  "displayName": "Jule",
  "description": "Jule programming language support for Visual Studio Code.",
  "publisher": "julelang",
  "version": "0.0.1",
  "icon": "https://github.com/julelang/resources/blob/main/jule_icon.png",
  "homepage": "https://jule.dev",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "Jule",
    "jule",
    "jule language",
    "julelang",
    "extension"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/julelang/vscode-jule"
  },
  "bugs": {
    "url": "https://github.com/julelang/vscode-jule/issues"
  },
  "contributes": {
    "snippets": [
      {
        "language": "jule",
        "path": "snippets/snippets.json"
      }
    ],
    "languages": [{
      "id": "jule",
      "aliases": ["Jule", "jule"],
      "extensions": [".jule"],
      "configuration": "languages/language-configuration.json",
      "icon": {
        "dark": "resources/jule_icon.svg",
        "light": "resources/jule_icon.svg"
      }
    }],
    "configuration": {
      "title": "Jule"
    },
    "breakpoints": [
      {
        "language": "jule"
      }
    ],
    "grammars": [{
      "language": "jule",
      "scopeName": "source.jule",
      "path": "./syntaxes/jule.tmLanguage.json"
    }]
  }
}
